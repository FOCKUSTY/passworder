{
  "name": "passworder",
  "version": "1.0.0-beta",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run pkg",
    "pkg": "npm run pkg:main && npm run pkg:au",
    "pkg:main": "pkg -t node*-win-x64 dist/main/index.js -o dist/main.exe",
    "pkg:au": "pkg -t node*-win-x64 dist/auto-updater/index.js -o dist/updater.exe",
    "start": "npm run build && npm run execute",
    "execute": "cd dist && main.exe",
    "execute:js": "node dist/main/index.js",
    "start:js": "npm run build && npm run execute:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FOCKUSTY/passworder.git"
  },
  "author": {
    "name": "FOCKUSTY",
    "email": "viserd.yt@gmail.com",
    "url": "https://fockusty.netlify.app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FOCKUSTY/passworder/issues"
  },
  "homepage": "https://github.com/FOCKUSTY/passworder#readme",
  "description": "",
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/tar-stream": "^3.1.4",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.35.0",
    "pkg": "^5.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  },
  "dependencies": {
    "random-js": "^2.1.0",
    "tar-stream": "^3.1.7"
  }
}
